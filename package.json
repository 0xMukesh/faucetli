  "dependencies": {
    "@oclif/command": "^1.8.16",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "axios": "^0.26.1",
    "chalk": "4.1.2",
    "figchalk": "^1.0.3",
    "inquirer": "^8.2.4",
